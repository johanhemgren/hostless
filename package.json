{
  "name": "hostless",
  "version": "0.0.0",
  "description": "Websites stored in the URL itself",
  "scripts": {
    "start": "cross-env NODE_ENV=testing npx sandbox",
    "build": "npm run build:public && npm run build:app",
    "build:public": "esbuild public/make.js --bundle --minify --sourcemap --outdir=public/dist",
    "build:app": "esbuild src-es/shared/*.js --minify --target=node12 --outdir=src/shared && esbuild src-es/http/**/*.js --minify --target=node12 --outdir=src/http"
  },
  "devDependencies": {
    "@architect/functions": "latest",
    "@architect/sandbox": "^3.3.6",
    "@begin/data": "latest",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0"
  },
  "dependencies": {
    "crypto-js": "^4.0.0",
    "esbuild": "^0.11.2",
    "lzutf8": "^0.6.0",
    "tldts": "^5.7.18"
  }
}
